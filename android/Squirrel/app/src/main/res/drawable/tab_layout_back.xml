<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:top="3dp"
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        >
        <shape>

            <solid android:color="@color/black2" />
            <!--        圆角设置-->
            <corners android:radius="15dp" />

            <!--        边框设置-->
            <stroke
                android:width="2dp"
                android:color="@color/white"
                />

        </shape>

    </item>



</layer-list>
