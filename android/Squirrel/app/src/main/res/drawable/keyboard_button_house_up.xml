<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item

        android:top="0dp"
        >
        <shape>

            <solid android:color="@color/button_high" />
            <!--        圆角设置-->
            <corners android:radius="5dp" />

            <!--        边框设置-->
            <stroke
                android:width="1dp"
                android:color="@color/button_frame"
                />

        </shape>

    </item>


    <item
        android:top ="2dp"
        android:bottom ="4dp"
        android:left ="2dp"
        android:right ="2dp"

        >
        <shape>
            <solid android:color="@color/button_start" />
            <!--        圆角设置-->
            <!--        <corners android:radius="5dp" />-->
            <corners
                android:topLeftRadius="5dp"
                android:topRightRadius="5dp"
                android:bottomLeftRadius="5dp"
                android:bottomRightRadius="5dp"
                />


            <gradient
                android:angle="90"
                android:centerX="100%"
                android:startColor="@color/button_start"
                android:centerColor="@color/button_center"
                android:endColor="@color/button_end"
                android:type="linear"
                />

        </shape>

    </item>

    <!-- 矢量图 -->
    <item>
        <vector
            android:width="24dp"
            android:height="24dp"
            android:viewportWidth="24"
            android:viewportHeight="24">
            <path
                android:fillColor="#ff74747c"
                android:pathData="M19,12h3L12,3L2,12h3v8h2v-2h10v2h2V12zM7.21,10h9.58L17,10.19V12H7v-1.81L7.21,10zM14.57,8H9.43L12,5.69L14.57,8zM7,16v-2h10v2H7z"/>
        </vector>
    </item>



</layer-list>
