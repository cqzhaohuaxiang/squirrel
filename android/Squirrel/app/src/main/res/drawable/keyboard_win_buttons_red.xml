<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:top="5dp"
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        >
        <shape>

            <solid android:color="@color/black" />
            <!--        圆角设置-->
            <corners android:radius="10dp" />

            <!--        边框设置-->
            <stroke
                android:width="1dp"
                android:color="@color/grey"
                />

        </shape>

    </item>

    <item
        android:top="6dp"
        android:bottom="6dp"
        android:left="6dp"
        android:right="6dp"
        >
        <shape>

            <solid android:color="@color/red" />
            <!--        圆角设置-->
            <corners android:radius="10dp" />

            <!--        边框设置-->
            <stroke
                android:width="1dp"
                android:color="@color/grey"
                />

        </shape>

    </item>

</layer-list>
