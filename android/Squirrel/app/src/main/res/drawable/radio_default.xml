<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item
        android:top="2dp"
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        >
        <shape>

            <solid android:color="@color/black" />
            <!--        圆角设置-->
            <corners android:radius="10dp" />

            <!--        边框设置-->
            <stroke
                android:width="1dp"
                android:color="@color/grey"
                />

        </shape>

    </item>



</layer-list>
